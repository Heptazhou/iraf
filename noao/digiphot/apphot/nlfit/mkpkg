# Non-linear fitting tools library.

$checkout libpkg.a ".."
$update libpkg.a
$checkin libpkg.a ".."
$exit

libpkg.a:
	nlinit.x	nlfitdef.h ../lib/nlfit.h
	nlacpts.x	nlfitdef.h ../lib/nlfit.h
	nliter.x	nlfitdef.h ../lib/nlfit.h <mach.h>
	nlfit.x		nlfitdef.h ../lib/nlfit.h <mach.h>
	nlvector.x	nlfitdef.h ../lib/nlfit.h
	nleval.x	nlfitdef.h ../lib/nlfit.h
	nlfree.x	nlfitdef.h
	nlsolve.x	nlfitdef.h ../lib/nlfit.h
	nlchomat.x	nlfitdef.h ../lib/nlfit.h <mach.h>
	nlpget.x	nlfitdef.h
	nlstat.x	nlfitdef.h ../lib/nlfit.h
	nlerrors.x	nlfitdef.h ../lib/nlfit.h <mach.h>
	;
