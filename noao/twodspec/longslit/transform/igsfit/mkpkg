# Interactive General Surface Fitting Package

$checkout libpkg.a ../../
$update	  libpkg.a
$checkin  libpkg.a ../../
$exit

libpkg.a:
	igscolon.x	igsfit.com
	igsdelete.x	<pkg/igsfit.h> <mach.h> <gset.h> <pkg/gtools.h>
	igsfit.x	<pkg/igsfit.h> <mach.h> <pkg/gtools.h>
	igsget.x	<pkg/igsfit.h> igsfit.com
	igsgraph.x	<pkg/igsfit.h> <mach.h> <gset.h> <pkg/gtools.h>
	igsinit.x	<pkg/igsfit.h>
	igsnearest.x	<pkg/igsfit.h> <mach.h> <gset.h>
	igsparams.x	igsfit.com <pkg/gtools.h>
	igsset.x	<pkg/igsfit.h> igsfit.com
	igssolve.x	igsfit.com <math/gsurfit.h>
	igsundelete.x	<pkg/igsfit.h> <mach.h> <gset.h> <pkg/gtools.h>
	xgs.x
	;
