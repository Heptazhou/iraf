# APEXTRACT

$call	relink
$exit

update:
	$call	relink
	$call	install
	;

relink:
	$update	libpkg.a
	$call	apextract
	;

install:
	$move	xx_apextract.e noaobin$x_apextract.e
	;

apextract:
	$omake	x_apextract.x
	$link	x_apextract.o libpkg.a -lxtools -lcurfit -liminterp\
		-o xx_apextract.e
	;

libpkg.a:
	apalloc.x	apertures.h
	apanswer.x	<pkg/xtanswer.h>
	apapertures.x	apertures.h
	apcenter.x	<pkg/center1d.h>
	apcolon.x	<error.h> <gset.h> <pkg/xtanswer.h> <imhdr.h>
	apcopy.x	apertures.h
	apcvset.x	apertures.h <math/curfit.h>
	apdb.x		apertures.h <math/curfit.h> <pkg/dttext.h>\
			<pkg/xtanswer.h>
	apdefault.x	apertures.h <imhdr.h>
	apdelete.x	
	apedit.x	apertures.h extract.h <gset.h> <imhdr.h> <mach.h>\
			<pkg/gtools.h> <pkg/xtanswer.h>
	apextract.x	apertures.h <error.h> <imhdr.h> <pkg/gtools.h>\
			<pkg/xtanswer.h>
	apfind.x	apertures.h <imhdr.h> <pkg/xtanswer.h>
	apfindnew.x	apertures.h
	apgapid.x	
	apgbeam.x	
	apgetdata.x	<imhdr.h>
	apgetim.x	
