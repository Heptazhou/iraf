# ONEDSPEC package.

$call	relink
$exit

update:
	$call	relink
	$call	install
	;

relink:
	$update	libpkg.a
	$omake	x_onedspec.x
	$link	x_onedspec.o libpkg.a -lxtools -lcurfit -lgsurfit -liminterp\
		-lllsq -o xx_onedspec.e
	;

install:
	$move	xx_onedspec.e noaobin$x_onedspec.e
	;

libpkg.a:
	@dispcor
	@ecidentify
	@fortran
	@identify
	@sensfunc
	@splot

	coincor.x	oned.h
	conversion.x	
	getairm.x	
	getcalib.x	oned.h <ctype.h>
	getextn.x	idsmtn.h oned.h
	getnimage.x	oned.h <ctype.h> <mach.h> <time.h> idsmtn.h
	idsm_keywrds.x	<mach.h> idsmtn.h <imhdr.h>
	load_hdr.x	idsmtn.h <imhdr.h>
	setdisp.x	<imhdr.h> <error.h>
	sflip.x		idsmtn.h <imhdr.h> <error.h> <fset.h>
	t_addsets.x	<mach.h> <ctype.h> <error.h> <imhdr.h> <fset.h>\
			oned.h idsmtn.h
	t_bswitch.x	idsmtn.h oned.h <ctype.h> <error.h> <fset.h> <imhdr.h>\
			<mach.h> <time.h>
	t_calibrate.x	<ctype.h> <error.h> <fset.h> <imhdr.h> <mach.h>\
			idsmtn.h oned.h
	t_coefs.x	idsmtn.h <ctype.h> <error.h> <fset.h> <imhdr.h>\
			<mach.h> oned.h
	t_coincor.x	idsmtn.h oned.h <ctype.h> <error.h> <fset.h> <imhdr.h>\
			<mach.h>
	t_combine.x	<ctype.h> <error.h> <fset.h> <imhdr.h> <mach.h>\
			<math/iminterp.h> idsmtn.h oned.h
	t_ecctm.x	idsmtn.h <imhdr.h> <error.h> <pkg/gtools.h>\
			<pkg/rg.h> <math/curfit.h>
	t_flatdiv.x	<error.h> <fset.h> <imhdr.h> idsmtn.h oned.h
	t_flatfit.x	idsmtn.h oned.h <fset.h> <gset.h> <imhdr.h>\
			<math/curfit.h>
	t_lcalib.x
	t_mkspec.x	<imhdr.h>
	t_msselect.x	<error.h> <imhdr.h>
	t_names.x
	t_rebin.x	<ctype.h> <error.h> <fset.h> <imhdr.h> <mach.h>\
			<math/iminterp.h> idsmtn.h oned.h
	t_sinterp.x	idsmtn.h oned.h <fset.h> <imhdr.h> <math/curfit.h>
	t_shedit.x	<imhdr.h> <error.h> oned.h idsmtn.h
	t_specplot.x	specplot.h <imhdr.h> <error.h> <gset.h> <mach.h>\
			<pkg/gtools.h>
	t_slist.x	<ctype.h> <error.h> <fset.h> <imhdr.h> <mach.h>\
			idsmtn.h oned.h
	t_standard.x	oned.h <ctype.h> <error.h> <imhdr.h> idsmtn.h <fset.h>\
			<mach.h>
	t_subsets.x	<ctype.h> <error.h> <fset.h> <imhdr.h> <mach.h>\
			idsmtn.h oned.h
	t_sums.x	idsmtn.h oned.h <fset.h> <imhdr.h>
	t_widstape.x	<fset.h> <imhdr.h> idsmtn.h <ctype.h> <fset.h> <mach.h>
	;
