# LAPACK library routines

update:
        $checkout       libxtools.a lib$
        $update         libxtools.a
        $checkin        libxtools.a lib$
        ;

libxtools.a:
	dgemm.f
	dger.f
	dgetf2.f
	dgetrf.f
	dgetrs.f
	dlaswp.f
	dscal.f
	dswap.f
	dtrsm.f
	idamax.f
	isamax.f
	lsame.f
	sgemm.f
	sger.f
	sgetf2.f
	sgetrf.f
	sgetrs.f
	slaswp.f
	sscal.f
	sswap.f
	strsm.f
	;
