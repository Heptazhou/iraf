# Library for the gtools package.

$checkout libxtools.a lib$
$update	  libxtools.a
$checkin  libxtools.a lib$
$exit

libxtools.a:
	gtascale.x	<mach.h> <pkg/gtools.h>
	gtcolon.x	<gset.h> <pkg/gtools.h>
	gtcopy.x	<pkg/gtools.h>
	gtctran.x
	gtcur.x
	gtcur1.x	<pkg/gtools.h>
	gtfree.x	<pkg/gtools.h>
	gtget.x		<gset.h> <pkg/gtools.h>
#	gthelp.x	<error.h>
	gtinit.x	<gset.h> <pkg/gtools.h>
	gtlabax.x	<gset.h> <pkg/gtools.h>
#	gtpage.x	<ttyset.h>
	gtplot.x	<gset.h> <pkg/gtools.h>
	gtvplot.x	<gset.h> <pkg/gtools.h>
	gtset.x		<gset.h> <pkg/gtools.h>
	gtswind.x	<gset.h> <pkg/gtools.h> <mach.h>
	gtwindow.x	<pkg/gtools.h>
	;
