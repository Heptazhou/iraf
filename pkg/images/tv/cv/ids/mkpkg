# Make the CV package library.

$checkout libpkg.a ../
$update   libpkg.a
$checkin  libpkg.a ../
$exit

libpkg.a:
	idscancel.x	ids.com ids.h <fset.h>
	idschars.x	ids.com ids.h
	idsclear.x	ids.com ids.h
	idsclose.x	ids.com ids.h
	idsclosews.x	ids.h ids.com
	idscround.x	ids.h <gki.h>
	idsdrawch.x	font.com font.h <gki.h> <gset.h> <math.h>
	idsescape.x	ids.com ids.h <gki.h>
	idsfa.x		ids.com ids.h
	idsfaset.x	ids.com ids.h <gki.h>
	idsflush.x	ids.com ids.h
	idsfont.x	ids.com ids.h <gki.h> <gset.h>
	idsgcell.x	<mach.h> ids.com ids.h <gki.h> <gset.h>
	idsgcur.x	ids.com ids.h
	idsinit.x	ids.com ids.h <ctype.h> <gki.h> <mach.h>
	idsline.x	ids.com ids.h <gset.h>
	idslutfill.x	<gki.h>
	idsopen.x	ids.com ids.h <gki.h>
	idsopenws.x	ids.com ids.h <error.h> <gki.h> <fset.h> <mach.h>
	idspcell.x	ids.com ids.h <gki.h> <gset.h>
	idspl.x		ids.com ids.h <gki.h>
	idsplset.x	ids.com ids.h <gki.h>
	idspm.x		ids.com ids.h <gki.h>
	idspmset.x	ids.com ids.h <gki.h>
	idspoint.x	ids.com ids.h <fset.h> <gki.h>
	idsreset.x	ids.com ids.h <gset.h> <gki.h>
	idsrestore.x	ids.com ids.h
	idssave.x	ids.com ids.h
	idsscur.x	
	idsstream.x	ids.com ids.h
	idstx.x		ids.com ids.h <gki.h> <gset.h> <math.h>
	idstxset.x	ids.com ids.h <gki.h> <gset.h>
	idsvector.x	ids.com ids.h <fset.h> <gki.h>
